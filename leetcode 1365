class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int[] frq = new int[101];
        for(int num : nums){
            frq[num]++;
        }
        int[] prefix = new int[101];
        for(int i = 1;i<101;i++){
            prefix[i] = prefix[i-1]+frq[i];
        }
        int[] res = new int[nums.length];
        for(int i = 0;i<nums.length;i++){
            res[i] = prefix[nums[i]] - frq[nums[i]];
        }
        return res;
    }
}
